cells:
  - kind: 2
    value: "SELECT PARAMETER,\r

      \t   COUNT(*) total,\r

      \t   SUM(DECODE(ISDEPRECATED, 'TRUE', 1, 0)) is_deprecated,\r

      \t   SUM(DECODE(ISDEPRECATED, 'FALSE', 1, 0)) is_not_deprecated\r

      FROM V$NLS_VALID_VALUES\r

      GROUP BY PARAMETER;"
    languageId: oracle-sql
  - kind: 1
    value: "#### LANGUAGE"
    languageId: markdown
  - kind: 2
    value: "SELECT *\r

      FROM V$NLS_VALID_VALUES\r

